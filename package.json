{
  "name": "watch",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "server-dev": "ts-node-dev --respawn --transpileOnly ./server/src/app.ts",
    "server-prod": "tsc && node ./dist/server/src/app.js",
    "tslint": "tslint --project tsconfig.json",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "start": "npm run build-client && npm run server-prod",
    "start-local": "npm run build-client && npm run server-dev",
    "build-client": "cd client && npm run build && cd .."
  },
  "author": "Dimitar Hristov",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "mssql": "^6.1.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.8.2"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0"
  }
}
